{
    "name": "reactmore-tech/support",
    "description": "Reactmore Tech Support, Helpers",
    "license": "MIT",
    "keywords": [
        "Helpers",
        "Guzzle"
    ],
    "homepage": "https://github.com/reactmore-tech/support",
    "type": "library",
    "minimum-stability": "stable",
    "authors": [
        {
            "name": "Reactmore",
            "homepage": "https://github.com/reactmore",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.0",
        "guzzlehttp/guzzle": "^7.0.1",
        "psr/http-message": "~1.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "squizlabs/php_codesniffer": "^3.7.2",
        "kint-php/kint": "^6.0"
    },
    "autoload": {
        "psr-4": {
            "ReactMoreTech\\Support\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ReactMoreTech\\Support\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "phpunit",
        "test_debug": "phpunit --debug"
    }
}
